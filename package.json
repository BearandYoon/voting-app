    "d3": "^4.8.0",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "mongoose": "^5.0.9",
    "morgan": "^1.8.1",
    "ng2-toastr": "^4.0.1",
    "passport": "^0.4.0",
