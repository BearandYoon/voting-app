{
  "name": "voting-app",
  "version": "1.0.0",
  "license": "MIT",
  "author": "",
  "description": "FCC voting app",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "postinstall": "tsc -p server && ng build -aot -prod",
    "dev": "concurrently \"mongod\" \"ng serve -pc proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon\"",
    "prod": "concurrently \"mongod\" \"ng build -aot -prod && tsc -p server && node dist/server/app.js\"",
    "test:client": "ng test",
    "test:server": "tsc -p server && cross-env NODE_ENV=test ./node_modules/.bin/mocha --timeout 15000 --ui bdd --reporter spec --colors dist/server/test/**",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.7",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.7",
